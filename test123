package stepdefinitions;

import cucumber.api.java.en.Given;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class MyFirstSteps {
	
	
	@Given("I am on the search page of my app")
	public void i_am_on_the_search_page_of_my_app() {
	    System.out.println("Step:I am on search page");
	}

	@When("I search for dress")
	public void i_search_for_dress() {
	    System.out.println("Step: search dress");
	}

	@Then("I see suggestions related to dress")
	public void i_see_suggestions_related_to_dress() {
	   System.out.println("Step: suggestions for dress");
	}


	@Given("I navigate to my home page")
	public void i_navigate_to_my_home_page() {
	  System.out.println("back ground step navigate");
	}

}
